[user]
	name = saekis
	email = s.saeki71@gmail.com
[core]
	excludesfile = /Users/saeki/.gitignore
	editor = /usr/bin/vim
[color]
	ui = true
[alias]
	co = checkout
	st = status
	br = branch
	logg = log --stat --pretty=format:'%Cblue%h%Creset %Cgreen%ar$Creset %Cred%an%Creset %s %Cred%d%Creset'
	loggr = log --graph --date-order --pretty=format:'%Cblue%h %Cgreen%ci %Cred%an %Cblue%m %Creset%s %Cred%d'
[url "git@github.com:"]
	pushinsteadof = https://github.com/
